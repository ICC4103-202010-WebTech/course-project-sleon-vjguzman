<h2 class="searchh2"> Search Results </h2>

<h3>Results by User</h3>
<% unless @search.users != nil %>
  <div class="div-2">
    <% @search.users.each do |user| %>
      <h4><%link_to user.name, user_path(user)%></h4>
    <%end%>
  </div>
<%end%>
<% unless @search.user == nil%>
  <div class="div-2">
      <h4>no results, try again</h4>
  </div>
<%end %>

