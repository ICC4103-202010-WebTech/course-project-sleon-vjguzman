<%= stylesheet_link_tag 'event-comments'%>
<br/>
<aside class="aside-comment">
  <button class="btn11 success-1" ><%= link_to 'Back', _back_url %></button>
</aside>
<br/>
<br/>

<h2>Dates: "<%= Event.where(id: @get_id).select(:title).first.title %>" Event:</h2>
<br/>
<% @event_dates.each do |x| %>
  <div>
    <h3 class="User-11"><%= x.date %></h3>
    <button class="btn11 danger2"><%= link_to 'Edit', edit_event_date_path(x)%></button>
    <button class="btn11 danger2"><%= link_to 'Delete', x,  method: :delete, data: { confirm: 'Are you sure?' }%></button>
  </div>
<% end %>

