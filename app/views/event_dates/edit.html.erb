<p id="notice"><%= notice %></p>
<div class="row ml-0 mt-3">
  <h2 class="edith2">Edit the Event Dates</h2>
</div>
<div class="div-2">
<%= form_with model: @event_date, local: true do |form| %>
  <div class="div-edit">
    <%= form.label :date %>
    <br/>
    <%= form.datetime_local_field :date %>
  </div>
  <div class="div-edit">
    <%= form.submit "Update", class: ["btn"] %>
  </div>
<%end %>
</div>
