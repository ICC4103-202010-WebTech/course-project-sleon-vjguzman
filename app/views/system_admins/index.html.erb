<p id="notice"><%= notice %></p>
<%= stylesheet_link_tag 'system-admin'%>
<h2 class="adminh3">Users with admin permissions</h2>
</br>
  <% @admins.each do |x| %>
    <div class="divadmin">
      <span class="imageleft"><%= link_to image_tag("xd.jpeg") %></span>
      <h4 class="adminh4p"><%= x.username %></h4>
      <h4 class="adminh4"><%= x.full_name %></h4>
      <h4 class="adminh4"><%= x.email %></h4>
      <button class="adminboton"><%= link_to 'View', user_path(x) %></button>
    </div>
  <% end %>

<button class="adminboton1"><%= link_to 'Back', system_admins_path %></button>
