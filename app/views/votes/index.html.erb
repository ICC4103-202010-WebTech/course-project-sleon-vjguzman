<p id="notice"><%= notice %></p>

<h2>Votes of the Event </h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @dates.each do |date| %>
      <tr>
        <td><%= date %></td>
        <% @dates.each do |vote| %>
VOTE.WHERE(DATE_ID : DATES_ID AND GUEST_ID: LIST_ID)

        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %>
