<!DOCTYPE html>
<html>
  <head>
    <title>Supreme Events</title>
    <h1><%= link_to 'Supreme Events', root_path %></h1>
    <h5 align="right"> <%=User.first.full_name%> </h5>
    <h5 align="right"> Log out </h5>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application'%>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <nav>
    <%= stylesheet_link_tag 'navegation'%>
    <ul>
      <li>
        <%= link_to 'Profile', users_path%>
      </li>
      <li>
        <%= link_to 'My Events', events_path %>
      </li>
      <li>
        <%= link_to 'Organization', organizations_path %>
      </li>
      <li>
        <%= link_to 'Admin', system_admins_path %>
      </li>
    </ul>
  </nav>

  <body>
    <%= yield %>
  </body>
</html>
