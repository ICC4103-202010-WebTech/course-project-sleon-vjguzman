<!DOCTYPE html>
<html>
<head>
  <title>Supreme Events</title>
  <h1 class="apph1"><%= link_to image_tag("asdfas real.jpg"), root_path %></h1>
  <%unless user_signed_in? == true %>
    <h5 class="apph5"><%= link_to "Sign In", user_session_path %></h5>
    <h5 class="apph5"><%= link_to "Sign Up", new_user_session_path %></h5>
  <% end %>
  <%unless user_signed_in? == false %>
  <h5 class="apph5"><%= link_to "Log Out", destroy_user_session_path %></h5>
  <%end %>

  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag 'application'%>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<nav>
  <%= stylesheet_link_tag 'navegation'%>
  <ul>
    <%unless user_signed_in? == false %>
    <li>
      <%= link_to 'Profile', users_path%>
    </li>
    <li>
      <%= link_to 'My Events', events_path %>
    </li>
    <li>
      <%= link_to 'Organization', organizations_path %>
    </li>
    <li>
      <%= link_to 'Mailbox', mailboxes_path %>
    </li>
    <li>
      <%= link_to 'Admin', system_admins_path %>
    </li>
    <%end %>
    <li>
      <%= link_to "Search", new_search_path%>
    </li>

  </ul>
</nav>

<body>
<%= yield %>
</body>
</html>