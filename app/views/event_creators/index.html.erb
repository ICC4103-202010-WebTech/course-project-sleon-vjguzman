<p id="notice"><%= notice %></p>

<h1>Event Creators</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_creators.each do |event_creator| %>
      <tr>
        <td><%= link_to 'Show', event_creator %></td>
        <td><%= link_to 'Edit', edit_event_creator_path(event_creator) %></td>
        <td><%= link_to 'Destroy', event_creator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Creator', new_event_creator_path %>
